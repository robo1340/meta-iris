# Mount points for a standard ares image

/dev/root        /               auto     defaults,ro                        1  1
proc             /proc           proc     defaults                           0  0
devpts           /dev/pts        devpts   mode=0620,gid=5                    0  0
tmpfs            /run            tmpfs    mode=0755,nodev,nosuid,strictatime 0  0
#tmpfs            /var/volatile   tmpfs    defaults                           0  0

#/dev/data1        /ares/var/data/                  auto     auto,nofail,noatime,rw,user        0  0
/dev/boot1        /mnt/boot/boot/                  auto     auto,nofail,noatime,rw,user        0  0
/dev/boot3        /ares/var/run/                   auto     auto,nofail,noatime,rw,user        0  0
/dev/boot5        /mnt/boot/verify/                auto     auto,nofail,noatime,rw,user        0  0
/dev/boot6        /var/volatile/                        auto     auto,nofail,noatime,rw,user        0  0
/dev/boot7        /mnt/boot/misc1/                 auto     auto,nofail,noatime,rw,user        0  0
/dev/boot8        /mnt/boot/misc2/                 auto     auto,nofail,noatime,rw,user        0  0

/dev/mmcblk0p1        /mnt/mmc/boot/                   auto     auto,nofail,noatime,rw,user        0  0
/dev/mmcblk0p2        /mnt/mmc/root/                   auto     auto,nofail,noatime,rw,user        0  0
/dev/mmcblk0p3        /mnt/mmc/recovery_state/         auto     auto,nofail,noatime,rw,user        0  0
/dev/mmcblk0p5        /mnt/mmc/recovery_boot_storage/  auto     auto,nofail,noatime,rw,user        0  0
/dev/mmcblk0p6        /mnt/mmc/recovery_root_storage/  auto     auto,nofail,noatime,rw,user        0  0
/dev/mmcblk0p7        /mnt/mmc/swu_boot_storage/       auto     auto,nofail,noatime,rw,user        0  0
/dev/mmcblk0p8        /mnt/mmc/swu_root_storage/       auto     auto,nofail,noatime,rw,user        0  0
/dev/mmcblk0p9        /mnt/mmc/misc1/                  auto     auto,nofail,noatime,rw,user        0  0
